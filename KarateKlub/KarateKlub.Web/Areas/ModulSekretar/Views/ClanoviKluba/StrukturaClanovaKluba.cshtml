                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasDjecaŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasKadetiM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasKadetiŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasJunioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasJunioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasSenioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.smedjiPojasSenioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojSmedjiPojas </td>

                                    </tr>
