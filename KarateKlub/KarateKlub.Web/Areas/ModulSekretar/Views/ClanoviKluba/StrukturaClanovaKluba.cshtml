                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasDjecaM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasDjecaŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasKadetiM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasKadetiŽ </td>
