                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasJunioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasSenioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.plaviPojasSenioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojPlaviPojas </td>

