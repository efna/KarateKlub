                                        <td class="col-xs-1" style="text-align:center;">@Model.zeleniPojasSenioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.zeleniPojasSenioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojZeleniPojas </td>

                                    </tr>
