                                        <td style="text-align:left;">Majstori karatea</td>

                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasDjecaM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasDjecaŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasKadetiM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasKadetiŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasJunioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasJunioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasSenioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasSenioriŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojCrniPojas </td>

                                    </tr>               
                                    <tr>
                                        <td style="text-align:left;">Ukupno:</td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojDjeceM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojDjeceŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojKadetaM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojKadetaŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.crniPojasJunioriM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojJunioraŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojSenioraM </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojSenioraŽ </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojClanova </td>

                                    </tr>             
                                       

                                    </tbody>
                            </table>
                            <br />
                            <table style="border-top: 1px solid rgba(0, 0, 0, 0.12); font-family:Arial;" class="table table-hover table-striped table-bordered" id="tabelaStrukturaClanova2">
                                <thead style="font-size:16px; font-weight:500;">
                                    <tr>
                                        <th></th>
                                        <th class="col-xs-1" style="text-align:center;">Djeca</th>
                                        <th class="col-xs-1" style="text-align:center;">Kadeti</th>
                                        <th class="col-xs-1" style="text-align:center;">Juniori</th>
                                        <th class="col-xs-1" style="text-align:center;">Seniori</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="col-xs-1" style="text-align:center;">Ukupno: </td> 
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojDjece </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojKadeta </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojJuniora </td>
                                        <td class="col-xs-1" style="text-align:center;">@Model.ukupanBrojSeniora </td>

                                    </tr>
                                  
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <br />



                </div>
            </div>

        </div>
    </div>

</div>

