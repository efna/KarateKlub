@model KarateKlub.Web.Areas.ModulSekretar.Models.ClanoviKlubaStrukturaClanovaKlubaVM
@using KarateKlub.Web.Areas.ModulSekretar.Models

@{
    ViewBag.PageTitle = "Struktura članova";
    ViewBag.Title = "Struktura članova";
    Layout = null;
    
}

<style>
    .poravnanje {
        width: 50px;
        margin-left: 5px;
        display: inline-block;
    }

    .poravnanje2 {
        width: 200px;
        margin-left: 5px;
        display: inline-block;
    }
</style>

